<p id="notice"><%= notice %></p>

<h1>Posts</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="6"></th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.title %></td>
        <td><%= link_to 'Show', post %></td>
        <td><%= link_to 'Edit [JQ Ready]', [:edit_ready, post] %></td>
        <td><%= link_to 'Edit [Inline]', [:edit_inline, post] %></td>
        <td><%= link_to 'Edit [With Cleanup]', [:edit_cleanup, post] %></td>
        <td><%= link_to 'Edit (Original Scaffold)', [:edit, post] %></td>
        <td><%= link_to 'Destroy', post, method: :delete, remote: true, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Post', url_for([:new, :post]) %> | 
<%= link_to 'See Live Traffic (with interval cleanup)', url_for(:traffic) %> |
<%= link_to 'See Live Traffic (without interval cleanup)', url_for([:show_wrong_interval, :traffic]) %>